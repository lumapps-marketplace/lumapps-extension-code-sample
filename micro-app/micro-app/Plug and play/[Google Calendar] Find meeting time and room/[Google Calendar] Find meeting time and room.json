              },
              {
                "id": "meetDesc",
                "type": "Input.Text",
                "label": "Description of the meeting  (optional)",
                "spacing": "None",
                "placeholder": "Enter the description of your meeting"
              },
              {
                "id": "needRoom",
                "type": "Input.Toggle",
                "title": "Book a room"
              }
            ]
          },
          {
            "type": "ActionSet",
            "actions": [
              {
                "type": "Action.Execute",
                "style": "positive",
                "title": "Next"
              }
            ]
          }
        ],
